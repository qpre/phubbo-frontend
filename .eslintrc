{
  "env": {
    "browser": true,
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "jsx":      true,
    "modules":  true
  },
  "extends": "airbnb",
  "rules": {
    "quotes": [2, "single"],
    "react/prop-types": 0,
    "prop-types": 0,
    "key-spacing":  0,
    "no-multi-spaces": 0,
    "jsx-quotes": 0,
    "no-console": 0,
    "consistent-return": 0,
    "prefer-arrow-callback": 0
  },
  "plugins": ["flowtype"]
}
